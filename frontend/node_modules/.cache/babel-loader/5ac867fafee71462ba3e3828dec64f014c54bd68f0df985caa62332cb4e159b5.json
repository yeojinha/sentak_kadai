{"ast":null,"code":"import { reactive } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  setup() {\n    const cardBody = null;\n    const state = reactive({\n      data: [],\n      selected_category: \"\",\n      selected_date: \"\",\n      list: []\n    });\n\n    // methdos\n\n    const add = () => {};\n    const hideCardBody = event => {\n      cardBody = event.target.closest(\".card\").querySelector(\".card-body\");\n      cardBody.classList.add(\"d-none\");\n    };\n    ///axios\n    axios.get(\"/api/todolist\").then(res => {\n      state.list = res.data;\n      console.log(state.list);\n    });\n    return {\n      state,\n      hideCardBody\n    };\n  }\n};","map":{"version":3,"names":["reactive","axios","setup","cardBody","state","data","selected_category","selected_date","list","add","hideCardBody","event","target","closest","querySelector","classList","get","then","res","console","log"],"sources":["E:\\files\\Coding\\udemy\\js_bootcamp\\test\\frontend\\src\\components\\TodoList.vue"],"sourcesContent":["<template>\r\n  <section class=\"vh-100\">\r\n    <div class=\"container py-5 h-100\">\r\n      <div class=\"row d-flex justify-content-center align-items-center h-100\">\r\n        <div class=\"col\">\r\n          <div\r\n            class=\"card\"\r\n            id=\"list1\"\r\n            style=\"border-radius: 0.75rem; background-color: #eff1f2\"\r\n          >\r\n            <div class=\"card-body py-4 px-4 px-md-5\">\r\n              <p class=\"h1 text-center mt-3 mb-4 pb-3 text-primary\">\r\n                <i class=\"fas fa-check-square me-1\"></i>\r\n                <u>JOIN CHALLENGE</u>\r\n              </p>\r\n              <!-- \r\n              <div class=\"pb-2\">\r\n                <div class=\"card\">\r\n                  <div class=\"card-body\">\r\n                    <div class=\"d-flex flex-row align-items-center\">\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control form-control-lg\"\r\n                        id=\"exampleFormControlInput1\"\r\n                        placeholder=\"Add new...\"\r\n                      />\r\n                     \r\n                      <a\r\n                        href=\"#!\"\r\n                        data-mdb-toggle=\"tooltip\"\r\n                        title=\"Set due date\"\r\n                        ><i class=\"fas fa-calendar-alt fa-lg me-3\"></i\r\n                      ></a>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div> -->\r\n              <form>\r\n                <div class=\"form-group\">\r\n                  <label for=\"exampleFormControlInput1\">Email address</label>\r\n                  <input\r\n                    type=\"email\"\r\n                    class=\"form-control\"\r\n                    id=\"exampleFormControlInput1\"\r\n                    placeholder=\"name@example.com\"\r\n                  />\r\n                </div>\r\n                <div class=\"form-group\">\r\n                  <label for=\"exampleFormControlSelect1\">Title</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"exampleFormControlSelect1\"\r\n                    placeholder=\"plz enter the title, this will be shown as your title\"\r\n                  />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"exampleFormControlTextarea1\"\r\n                    >Example textarea</label\r\n                  >\r\n                  <textarea\r\n                    class=\"form-control\"\r\n                    id=\"exampleFormControlTextarea1\"\r\n                    placeholder=\"event detail place\"\r\n                    rows=\"3\"\r\n                  ></textarea>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                  <label for=\"exampleInputPassword1\">Password</label>\r\n                  <input\r\n                    type=\"password\"\r\n                    class=\"form-control\"\r\n                    id=\"exampleInputPassword1\"\r\n                    placeholder=\"Password\"\r\n                  />\r\n                </div>\r\n                <div>\r\n                  <button type=\"button\" class=\"btn btn-primary\" @click=\"add()\">\r\n                    Add\r\n                  </button>\r\n                </div>\r\n              </form>\r\n              <!-- \r\n              <div>\r\n                        <button\r\n                          type=\"button\"\r\n                          class=\"btn btn-primary\"\r\n                          @click=\"add()\"\r\n                        >\r\n                          Add\r\n                        </button>\r\n                      </div> -->\r\n              <hr class=\"my-4\" />\r\n\r\n              <div\r\n                class=\"d-flex justify-content-end align-items-center mb-4 pt-2 pb-3\"\r\n              >\r\n                <p class=\"small mb-0 me-2 text-muted\">Filter</p>\r\n                <select class=\"select\" v-model=\"state.selected_category\">\r\n                  <option v-for=\"(item, index) in state.data\" :key=\"index\">\r\n                    {{ item }}\r\n                  </option>\r\n                </select>\r\n                <p class=\"small mb-0 ms-4 me-2 text-muted\">Sort</p>\r\n                <select class=\"select\" v-model=\"state.selected_date\">\r\n                  <option value=\"1\">Added date</option>\r\n                  <option value=\"2\">Due date</option>\r\n                </select>\r\n                <a\r\n                  href=\"#!\"\r\n                  style=\"color: #23af89\"\r\n                  data-mdb-toggle=\"tooltip\"\r\n                  title=\"Ascending\"\r\n                  ><i class=\"fas fa-sort-amount-down-alt ms-2\"></i\r\n                ></a>\r\n              </div>\r\n\r\n              <!--  ul Challenge List -->\r\n              <div>\r\n                <div\r\n                  class=\"card\"\r\n                  v-for=\"(item, index) in state.list\"\r\n                  :key=\"{ index }\"\r\n                >\r\n                  <div class=\"card-header\" id=\"headingOne\">\r\n                    <h5 class=\"mb-0\">\r\n                      <button\r\n                        class=\"btn btn-link\"\r\n                        data-toggle=\"collapse\"\r\n                        data-target=\"#collapseOne\"\r\n                        aria-expanded=\"false\"\r\n                        aria-controls=\"collapseOne\"\r\n                        v-on:click=\"hideCardBody\"\r\n                      >\r\n                        {{ item.title }}\r\n                      </button>\r\n                    </h5>\r\n                  </div>\r\n\r\n                  <div\r\n                    id=\"collapseOne\"\r\n                    class=\"collapse show\"\r\n                    aria-labelledby=\"headingOne\"\r\n                    data-parent=\"#accordion\"\r\n                  >\r\n                    <div class=\"card-body d-none\">\r\n                      {{ item.content }}\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n\r\n                <!-- <li\r\n                    class=\"list-group-item d-flex align-items-center ps-0 pe-3 py-1 rounded-0 border-0 bg-transparent\"\r\n                  >\r\n                    <div class=\"form-check\">\r\n                      <input\r\n                        class=\"form-check-input me-0\"\r\n                        type=\"checkbox\"\r\n                        value=\"\"\r\n                        id=\"flexCheckChecked1\"\r\n                        aria-label=\"...\"\r\n                      />\r\n                    </div>\r\n                  </li>\r\n\r\n                  <li\r\n                    class=\"list-group-item px-3 py-1 d-flex align-items-center flex-grow-1 border-0 bg-transparent\"\r\n                  >\r\n                    <h5 class=\"mb-0\">\r\n                      <button\r\n                        class=\"btn btn-link collapsed\"\r\n                        data-toggle=\"collapse\"\r\n                        data-target=\"#collapseThree\"\r\n                        aria-expanded=\"false\"\r\n                        aria-controls=\"collapseThree\"\r\n                      >\r\n                        {{ item.title }}\r\n                      </button>\r\n                    </h5>\r\n                  </li>\r\n                  <li\r\n                    class=\"list-group-item ps-3 pe-0 py-1 rounded-0 border-0 bg-transparent\"\r\n                  >\r\n                    <div class=\"d-flex flex-row justify-content-end mb-1\">\r\n                      <a\r\n                        href=\"#!\"\r\n                        class=\"text-info\"\r\n                        data-mdb-toggle=\"tooltip\"\r\n                        title=\"Edit todo\"\r\n                        ><i class=\"fas fa-pencil-alt me-3\"></i\r\n                      ></a>\r\n                      <a\r\n                        href=\"#!\"\r\n                        class=\"text-danger\"\r\n                        data-mdb-toggle=\"tooltip\"\r\n                        title=\"Delete todo\"\r\n                        ><i class=\"fas fa-trash-alt\"></i\r\n                      ></a>\r\n                    </div>\r\n                    <div class=\"text-end text-muted\">\r\n                      <a\r\n                        href=\"#!\"\r\n                        class=\"text-muted\"\r\n                        data-mdb-toggle=\"tooltip\"\r\n                        title=\"Created date\"\r\n                      >\r\n                        <p class=\"small mb-0\">\r\n                          <i class=\"fas fa-info-circle me-2\"></i\r\n                          >{{ item.createdAt }}\r\n                        </p>\r\n                      </a>\r\n                    </div>\r\n                  </li>\r\n                -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from \"vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  setup() {\r\n    const cardBody = null;\r\n    const state = reactive({\r\n      data: [],\r\n      selected_category: \"\",\r\n      selected_date: \"\",\r\n      list: [],\r\n    });\r\n\r\n    // methdos\r\n\r\n    const add = () => {};\r\n    const hideCardBody = (event) => {\r\n      cardBody = event.target.closest(\".card\").querySelector(\".card-body\");\r\n\r\n      cardBody.classList.add(\"d-none\");\r\n    };\r\n    ///axios\r\n    axios.get(\"/api/todolist\").then((res) => {\r\n      state.list = res.data;\r\n      console.log(state.list);\r\n    });\r\n\r\n    return { state, hideCardBody };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n#list1 .form-control {\r\n  border-color: transparent;\r\n}\r\n#list1 .form-control:focus {\r\n  border-color: transparent;\r\n  box-shadow: none;\r\n}\r\n#list1 .select-input.form-control[readonly]:not([disabled]) {\r\n  background-color: #fbfbfb;\r\n}\r\n.hide {\r\n  visibility: hidden;\r\n}\r\n</style>\r\n"],"mappings":"AAiOA,SAASA,QAAO,QAAS,KAAK;AAC9B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAI,IAAI;IACrB,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;MACrBK,IAAI,EAAE,EAAE;MACRC,iBAAiB,EAAE,EAAE;MACrBC,aAAa,EAAE,EAAE;MACjBC,IAAI,EAAE;IACR,CAAC,CAAC;;IAEF;;IAEA,MAAMC,GAAE,GAAIA,CAAA,KAAM,CAAC,CAAC;IACpB,MAAMC,YAAW,GAAKC,KAAK,IAAK;MAC9BR,QAAO,GAAIQ,KAAK,CAACC,MAAM,CAACC,OAAO,CAAC,OAAO,CAAC,CAACC,aAAa,CAAC,YAAY,CAAC;MAEpEX,QAAQ,CAACY,SAAS,CAACN,GAAG,CAAC,QAAQ,CAAC;IAClC,CAAC;IACD;IACAR,KAAK,CAACe,GAAG,CAAC,eAAe,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;MACvCd,KAAK,CAACI,IAAG,GAAIU,GAAG,CAACb,IAAI;MACrBc,OAAO,CAACC,GAAG,CAAChB,KAAK,CAACI,IAAI,CAAC;IACzB,CAAC,CAAC;IAE
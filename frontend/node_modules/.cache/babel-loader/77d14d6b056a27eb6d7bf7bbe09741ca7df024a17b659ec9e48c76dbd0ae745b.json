small mb-0\">\r\n                              <i class=\"fas fa-info-circle me-2\"></i\r\n                              >{{ item.createdAt }}\r\n                            </p>\r\n                          </a>\r\n                        </section>\r\n                      </li>\r\n                      <button\r\n                        class=\"btn btn-link\"\r\n                        data-toggle=\"'collapse' + index\"\r\n                        data-target=\"'#collapseOne' + index\"\r\n                        aria-expanded=\"'false' + index\"\r\n                        aria-controls=\"'collapseOne'  + index\"\r\n                        @click=\"hideCardBody(item.id)\"\r\n                      >\r\n                        {{ item.title }}\r\n                      </button>\r\n                    </h5>\r\n                  </section>\r\n\r\n                  <div\r\n                    id=\"'collapseOne' + index\"\r\n                    class=\"'collapse show' + index\"\r\n                    aria-labelledby=\"'headingOne' + index\"\r\n                    data-parent=\"#accordion\"\r\n                  >\r\n                    <!-- <div class=\"card-body d-none\"> -->\r\n                    <div class=\"card-body\" :class=\"{ 'd-none': item.isActive }\">\r\n                      {{ item.content }}\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n\r\n                <!-- <li\r\n                    class=\"list-group-item d-flex align-items-center ps-0 pe-3 py-1 rounded-0 border-0 bg-transparent\"\r\n                  >\r\n                    <div class=\"form-check\">\r\n                      <input\r\n                        class=\"form-check-input me-0\"\r\n                        type=\"checkbox\"\r\n                        value=\"\"\r\n                        id=\"flexCheckChecked1\"\r\n                        aria-label=\"...\"\r\n                      />\r\n                    </div>\r\n                  </li>\r\n\r\n                  <li\r\n                    class=\"list-group-item px-3 py-1 d-flex align-items-center flex-grow-1 border-0 bg-transparent\"\r\n                  >\r\n                    <h5 class=\"mb-0\">\r\n                      <button\r\n                        class=\"btn btn-link collapsed\"\r\n                        data-toggle=\"collapse\"\r\n                        data-target=\"#collapseThree\"\r\n                        aria-expanded=\"false\"\r\n                        aria-controls=\"collapseThree\"\r\n                      >\r\n                        {{ item.title }}\r\n                      </button>\r\n                    </h5>\r\n                  </li>\r\n                 \r\n                -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { reactive } from \"vue\";\r\nimport axios from \"axios\";\r\nimport { assertTSExpressionWithTypeArguments } from \"@babel/types\";\r\nexport default {\r\n  setup() {\r\n    const cardBody = null;\r\n    const state = reactive({\r\n      data: [],\r\n      selected_category: \"\",\r\n      selected_date: \"\",\r\n      list: [],\r\n      isActive: true,\r\n    });\r\n\r\n    // methdos\r\n\r\n    //추가\r\n    const add = () => {};\r\n\r\n    //hide and show button\r\n\r\n    const hideCardBody = (id) => {\r\n      console.log(\"id: \" + id);\r\n\r\n      state.isActive = !state.isActive; //true -> false, false -> true\r\n      axios\r\n        .put(\"/api/todolist\", {\r\n          isActive,\r\n          id,\r\n        })\r\n        .then((res) => {\r\n          state.list = res.data;\r\n        });\r\n      console.log(\"after active change: \" + state.list);\r\n      //\r\n    };\r\n\r\n    // const hideCardBody = (event) => {\r\n    //   const cardBody = event.target\r\n    //     .closest(\".card\")\r\n    //     .querySelector(\".card-body\");\r\n    //   cardBody.classList.toggle(\"d-none\");\r\n    // };\r\n    // ///axios\r\n    axios.get(\"/api/todolist\").then((res) => {\r\n      state.list = res.data;\r\n      console.log(state.list);\r\n    });\r\n\r\n    return { state, add, hideCardBody };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n#list1 .form-control {\r\n  border-color: transparent;\r\n}\r\n#list1 .form-control:focus {\r\n  border-color: transparent;\r\n  box-shadow: none;\r\n}\r\n#list1 .select-input.form-control[readonly]:not([disabled]) {\r\n  background-color: #fbfbfb;\r\n}\r\n.hide {\r\n  visibility: hidden;\r\n}\r\n</style>\r\n"],"mappings":"AA4NA,SAASA,QAAO,QAAS,KAAK;AAC9B,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,mCAAkC,QAAS,cAAc;AAClE,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAI,IAAI;IACrB,MAAMC,KAAI,GAAIL,QAAQ,CAAC;MACrBM,IAAI,EAAE,EAAE;MACRC,iBAAiB,EAAE,EAAE;MACrBC,aAAa,EAAE,EAAE;MACjBC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE;IACZ,CAAC,CAAC;;IAEF;;IAEA;IACA,MAAMC,GAAE,GAAIA,CAAA,KAAM,CAAC,CAAC;;IAEpB;;IAEA,MAAMC,YAAW,GAAKC,EAAE,IAAK;MAC3BC,OAAO,CAACC,GAAG,CAAC,MAAK,GAAIF,EAAE,CAAC;MAExBR,KAAK,CAACK,QAAO,GAAI,CAACL,KAAK,CAACK,QAAQ,EAAE;MAClCT,KAAI,CACDe,GAAG,CAAC,eAAe,EAAE;QACpBN,QAAQ;QACRG;MACF,CAAC,EACAI,IAAI,CAAEC,GAAG,IAAK;QACbb,KAAK,CAACI,IAAG,GAAIS,GAAG,CAACZ,IAAI;MACvB,CAAC,CAAC;MACJQ,OAAO,CAACC,GAAG,CAAC,uBAAsB,GAAIV,KAAK,CAACI,IAAI,CAAC;MACjD;IACF,CAAC;;IAED;IACA;IACA;IACA;IACA;IACA;IACA;IACAR,KAAK,CAACkB,GAAG,CAAC,eAAe,CAAC,CAACF,IAAI,CAAEC,GAAG,IAAK;MACvCb,KAAK,CAACI,IAAG,GAAIS,GAAG,CAACZ,IAAI;MACrBQ,OAAO,CAACC,GAAG,CAACV,KAAK,CAACI,IAAI,CAAC;IACzB,CAAC,CAAC;IAEF,OAAO;MAAEJ,KAAK;MAAEM,GAAG;MAAEC;IAAa,CAAC;EACrC;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}